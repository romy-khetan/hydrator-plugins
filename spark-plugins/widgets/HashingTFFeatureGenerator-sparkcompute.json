{
  "metadata": {
    "spec-version": "1.0"
  },
  "configuration-groups": [
    {
      "label": "Hashing TF Feature Generator Properties",
      "properties": [
        {
          "widget-type": "textbox",
          "label": "Pattern to split the input string fields on",
          "name": "pattern"
        },
        {
          "widget-type": "number",
          "label": "Number of features",
          "name": "numFeatures",
          "widget-attributes": {
            "default": 1048576,
            "min": 1
          }
        },
        {
          "widget-type": "keyvalue",
          "label": "Fields to Transform",
          "name": "outputColumnMapping",
          "widget-attributes": {
            "showDelimiter": "false"
          }
        }
      ]
    }
  ],
  "outputs": []
}
